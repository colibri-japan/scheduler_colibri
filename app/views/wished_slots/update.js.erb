$('.modal').modal('hide');

<% if @wished_slot.errors.any? %>
  alert('エラーが発生したため、シフト希望が編集されませんでした')
<% else %>
  if (typeof calendar === 'undefined') {
    let calendar;
  }
  if ($('.master_calendar').length > 0) {
    calendar = $('.master_calendar');
  }

  calendar.fullCalendar('removeEvents', '<%= "wished_slot_#{@wished_slot.id}" %>')

  if (window.nurseId == <%= @wished_slot.nurse_id %>) {
    calendar.fullCalendar(
      'renderEvents',
      $.parseJSON("<%= j render(partial: 'wished_slots/wished_slot.json', locals: {wished_slot: @wished_slot}).html_safe  %>"), 
    )
  }
<% end %>
