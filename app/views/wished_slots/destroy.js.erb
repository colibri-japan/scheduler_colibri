$('.modal').modal('hide');
$('.modal-backdrop').remove();

  if (window.fullCalendar) {
    events = window.fullCalendar.getEvents()
    for (let event of events) {
      window.fullCalendar.batchRendering(function(){
        if (event.id === '<%= j "wished_slot_#{@wished_slot.id}" %>') {
          event.remove()
        }
      })
    }
  }