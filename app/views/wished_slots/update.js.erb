$('.nurse_calendar').fullCalendar('removeEvents', '<%= "wished_slot_#{@wished_slot.id}" %>');
$('.modal').modal('hide');

$('.nurse_calendar').fullCalendar(
  'renderEvents', 
  $.parseJSON("<%= j render(partial: 'wished_slots/wished_slot.json', locals: {wished_slot: @wished_slot}).html_safe %>"), 
  false
);
