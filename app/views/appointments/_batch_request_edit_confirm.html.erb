<div id="batch_request_edit_confirm" class="modal fade">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h1 class="colibri-small-title">調整中リスト追加の確認</h1>
				<button type="button" class="close" data-dismiss="modal">&times;</button>
			</div>

			<div class="modal-body">

            <%= render partial: 'batch_action_appointments_table', locals: {appointments: @appointments} %>

                <div>
                    <div class="btn btn-danger text-white" id="batch-request-edit-confirm-submit" data-submit-path="<%= appointments_batch_request_edit_path %>">一斉に調整中にする</div>
                </div>
			</div>
		</div>
	</div>

</div>