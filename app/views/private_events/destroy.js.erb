$('.modal').modal('hide');

if (window.fullCalendar) {
  let event = window.fullCalendar.getEventById('<%= "private_event_#{@private_event.id}" %>')
  if (event) {
    event.remove()
  }
}

if ($('.activity_back_icon').length > 0) {
  $('#remote_container').empty()
  $('#detail-container').show() 
}
